{
  "name": "launchpad-v3-contracts",
  "version": "0.1.0",
  "private": true,
  "description": "Smart contract of the Launchpad V3 protocol",
  "license": "MIT",
  "author": {
    "name": "Singularity-DAO",
    "url": "https://github.com/Singularity-DAO"
  },
  "scripts": {
    "chain": "hardhat node --no-deploy --export-all ./generated/chain.json --network hardhat",
    "clean": "shx rm -rf ./artifacts ./cache ./out ./coverage ./coverage.json ./typechain-types",
    "compile": "yarn run compile:forge && yarn run compile:hardhat",
    "compile:forge": "forge build",
    "compile:hardhat": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && yarn typechain",
    "deploy": "yarn deploy:hardhat-deploy --network hardhat",
    "deploy:hardhat-deploy": "hardhat deploy --export-all ./deployments/hardhat_contracts.json",
    "deploy:network": "yarn deploy:hardhat-deploy --network",
    "fork": "hardhat node --network hardhat --fork",
    "postinstall": "DOTENV_CONFIG_PATH=./.env.example yarn typechain",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && yarn prettier:write && yarn lint:sol --fix --noPrompt",
    "lint:sol": "forge fmt --check && yarn solhint \"{script,contracts,test}/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "networks": "hardhat networks",
    "prettier:check": "prettier --check \"**/*.{js,json,md,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,ts,yml}\"",
    "test": "yarn test:forge && yarn test:hardhat",
    "test:forge": "forge test",
    "test:hardhat": "hardhat test",
    "test:hardhat:fulltrace": "hardhat test --fulltrace",
    "test:hardhat:gas": "REPORT_GAS=true CONTRACT_SIZER=true hardhat test",
    "test:hardhat:trace": "hardhat test --trace",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "verify": "hardhat run scripts/verify.ts --network"
  },
  "resolutions": {
    "ethers": "^5.7.2",
    "hardhat-deploy": "^0.12.1"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/solidity": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@ethersproject/units": "^5.7.0",
    "@ethersproject/wallet": "^5.7.0",
    "@ethersproject/wordlists": "^5.7.0",
    "@layerzerolabs/lz-definitions": "^2.3.39",
    "@layerzerolabs/lz-evm-messagelib-v2": "^2.3.39",
    "@layerzerolabs/lz-evm-protocol-v2": "^2.3.39",
    "@layerzerolabs/lz-evm-v1-0.7": "^2.3.39",
    "@layerzerolabs/lz-v2-utilities": "^2.3.39",
    "@layerzerolabs/oapp-evm": "^0.0.4",
    "@layerzerolabs/test-devtools-evm-foundry": "~0.2.12",
    "@layerzerolabs/toolbox-foundry": "~0.1.9",
    "@layerzerolabs/toolbox-hardhat": "~0.4.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-foundry": "^1.1.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "^5.0.2",
    "@primitivefi/hardhat-dodoc": "^0.2.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typechain/ethers-v5": "^11.1.2",
    "hardhat-storage-layout": "^0.1.7",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "~18.18.14",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "chai": "^4.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "ethers": "^5.7.2",
    "evm-bn": "^1.1.2",
    "hardhat": "^2.22.10",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.12.1",
    "hardhat-gas-reporter": "^2.2.1",
    "hardhat-preprocessor": "^0.1.5",
    "mocha": "^10.2.0",
    "prettier": "^3.3.3",
    "solhint": "^5.0.3",
    "solidity-bytes-utils": "^0.8.2",
    "solidity-coverage": "^0.8.13",
    "ts-node": "^10.9.2",
    "shx": "0.3.4",
    "typechain": "^8.3.2",
    "typescript": "^5.4.4"
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
